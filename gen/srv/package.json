{
  "name": "spacefarer",
  "dependencies": {
    "@cap-js/sqlite": "^2.1.3",
    "@sap/cds": "^9"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite"
      }
    },
    "watch": {
      "open": "app/spacefarer/app/launchpad/webapp/index.html"
    }
  },
  "devDependencies": {
    "ui5-middleware-simpleproxy": "^3.6.2"
  },
  "scripts": {
    "watch-project1": "cds watch --open project1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-spacefarer": "cds watch --open spacefarer/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-aldi.fiori.ui": "cds watch --open aldi.fiori.ui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-aldi.spacefarer.ui": "cds watch --open aldi.spacefarer.ui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-aldi.department.ui": "cds watch --open aldi.department.ui/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "sapux": [
    "app/project1",
    "app/spacefarer",
    "app/spacefarer",
    "app/aldi.fiori.ui",
    "app/aldi.spacefarer.ui",
    "app/aldi.department.ui",
    "app/aldi.department.ui"
  ],
  "engines": {
    "node": ">=20"
  }
}